pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/1ms24mc111/devops.git'
            }
        }
        stage('Build') {
            steps {
                sh 'javac Demo.java'
            }
        }
        stage('Run') {
            steps {
                sh 'java Demo'
            }
        }
    }
}
