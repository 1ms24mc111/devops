pipeline{
  agent any

  stages{

    stage('checkout'){
        steps {
            git branch: 'main', url: 'https://github.com/1ms24mc111/devops'
        }
    }

    stage('build') {
      steps {
        echo "Building...."
        sh 'python3 test.py'
      }
    }

    stage('Test') {
      steps {
        echo "Testing...."
      }
    }

  }


}
