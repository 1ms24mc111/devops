pipeline{
  agent any

  stages{

    stage('checkout'){
        steps {
            git branch: 'main', url: 'https://github.com/1ms24mc111/devops'
        }
    }

     stage('Build Java') {
            steps {
                sh 'javac Demo.java'
            }
        }
        stage('Run Java') {
            steps {
                sh 'java Demo'
            }
        }
   

    stage('Test') {
      steps {
        echo "Testing...."
      }
    }

  }


}
