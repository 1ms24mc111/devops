pipeline{
  agent any

tools{
  jdk 'jdk21'
}
  stages{

    stage('checkout'){
        steps {
            git branch: 'main', url: 'https://github.com/1ms24mc111/devops'
        }
    }

      stages {
        stage('Build') {
            steps {
                sh 'javac Demo.java'
            }
        }
        stage('Run') {
            steps {
                sh 'java Demo'
            }
        }
    }
   

    stage('Test') {
      steps {
        echo "Testing...."
      }
    }

  }


}
